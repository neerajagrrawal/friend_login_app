<h1 class="mt-4">Friends- </h1>
<% include ./partials/messages %>
<main role="main">
  <table class='table table-striped'>
    <thead>
        <tr>
          <th>Id</th>
          <th>First Name</th>
          <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
      <% users.forEach(function(user){ %>
        <tr id= "<%= user.id %>"  >
          <td><%= user.id %></td>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>


        </tr>
      <% }) ; %>
    </tbody>

  </table>
  </main>
